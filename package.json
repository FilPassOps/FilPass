{
  "name": "pl-disbursement",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "postinstall": "npx hardhat compile",
    "build": "next build && tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "dev": "next dev",
    "dev:server": "nodemon",
    "start": "npx prisma generate && node dist/server/index.js",
    "start:dev": "cross-env NODE_ENV=production IS_DEV=false node dist/server/index.js",
    "create-migration": "npx prisma migrate dev --create-only",
    "migrate": "prisma generate && prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "reset-db": "npx prisma migrate reset",
    "seed": "ts-node ./prisma/seed.ts",
    "seed:dev": "ts-node ./prisma/seedDev.ts",
    "docker:up": "docker-compose -f docker-compose.test.yml up -d ",
    "docker:down": "docker-compose -f docker-compose.test.yml down ",
    "test": "npm run docker:up && dotenv -e .env.test -- npm run migrate:test && dotenv -e .env.test -- jest -i --watch --forceExit",
    "test:coverage": "npm run docker:up && dotenv -e .env.test -- npm run migrate:test && dotenv -e .env.test -- jest -i --coverage --forceExit && npm run docker:down",
    "deploy": "npx hardhat run --network filecoin scripts/deploy.ts",
    "test-contract": "npx hardhat --network calibration test",
    "pre-dev": "docker compose up -d && npm run migrate && npm run seed && npm run seed:dev"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "resolutions": {
    "@types/react": "17.0.36"
  },
